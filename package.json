{
  "name": "tb-frontend",
  "version": "0.1.0",
  "description": "Frontend for tennis-billiard booking project",
  "scripts": {
    "dev": "babel-node tools/devServer.js",
    "dev:webpack": "webpack-dev-server --hot --inline --progress --colors --devtool eval --env=develop --port=8000",
    "lint": "esw webpack.config.* ./tools ./src ./test --ext .js --ext .jsx",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js \"./test/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist lint test",
    "build": "babel-node tools/build.js",
    "start": "babel-node tools/prodServer.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "bootstrap": "^3.3.7",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "jquery": "^3.1.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "toastr": "^2.1.2",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "colors": "^1.1.2",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.2",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-watch": "^2.1.14",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "nock": "^9.0.2",
    "node-sass": "^4.1.1",
    "npm-run-all": "^3.1.2",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^1.3.1",
    "redux-immutable-state-invariant": "^1.2.4",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-encoding-plugin": "^0.2.0",
    "webpack-hot-middleware": "^2.13.2"
  },
  "main": "webpack.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tb-booking/tb-frontend.git"
  },
  "author": "tb-booking",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tb-booking/tb-frontend/issues"
  },
  "homepage": "https://github.com/tb-booking/tb-frontend#readme"
}
